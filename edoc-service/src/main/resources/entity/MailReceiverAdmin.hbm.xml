<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping">

<hibernate-mapping>
    <class name="com.bkav.edoc.service.database.entity.MailReceiverAdmin" table="mail_receiver_admin">
        <id name="id" type="java.lang.Integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="name" type="string" column="name"/>
        <property name="emailAddress" type="string" column="email_address"/>
        <property name="createDate" type="timestamp" column="create_date"/>
        <property name="modifiedDate" type="timestamp" column="modified_date"/>
        <property name="status" type="boolean" column="status"/>
    </class>
</hibernate-mapping>